Description='A wireless connection for USC Secure Wireless'
Interface=eth0
Connection=wireless
Security=wpa-configsection
IP=dhcp
WPAConfigSection=(
    'ssid="USC Secure Wireless"'
    'key_mgmt=WPA-EAP'
    'eap=PEAP'
    'identity="usc-email-id"'
    'password="usc-password"'
    'priority=1'
    'phase2="auth=MSCHAPV2"'
)
